                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "503": {
                        "description": "Service unavailable"
                    }
                }
            }
        },
        "/v1/seats": {
            "post": {
                "tags": [
                    "seats"
                ],
                "description": "Method that obtains the seat availability at the specified busses.\n",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "Created user object",
